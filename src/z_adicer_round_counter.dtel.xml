     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>Z_ADICER_ROUND_COUNTER</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00001</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Der Wurfzähler wird der Strategie mitgegeben und zeigt an, welcher Wurf</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>innerhalb einer Runde gerade übergeben wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Der erste Wurf übergibt &quot;1&quot; als Wert, danach &quot;2&quot; und zuletzt &quot;3&quot;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Weitere Würfe sind nicht möglich.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
